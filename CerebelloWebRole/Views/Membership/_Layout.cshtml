<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>@ViewBag.Title</title>
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.maskedinput-1.3.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/scripts-site.js")" type="text/javascript"></script>
    <link href="@Url.Content("/Content/Styles/Reset.css")" rel="Stylesheet" type="text/css"  />
    <link href="@Url.Content("/Content/Styles/Common.css")"  rel="Stylesheet" type="text/css" />
    <link href="@Url.Content("/Content/Styles/Site/Site.css")" rel="Stylesheet" type="text/css" />
    <link href="@Url.Content("/Content/Styles/Site/Documents.css")" rel="Stylesheet" type="text/css" />
    <link href="@Url.Content("/Content/Styles/Site/Membership.css")" rel="Stylesheet" type="text/css" />
</head>
<body>
    <div class="header">
        <div class="wrapper-950">
            <a href="@Url.Action("Index", "Home")">
                <img alt="" src="@Url.Content("~/Content/Images/top-logo.png")" />
            </a>
        </div>
    </div>
    <div class="wrapper-950">
        <div class="main-column">
            <div class="document">
                <div class="document-header">
                    @RenderSection("DocumentTitle")
                </div>
                <div class="document-body">
                    @RenderSection("DocumentBody")
                </div>
                <div style="clear:both"></div>
            </div>
        </div>
        <div class="side-column">
            @RenderSection("SideBar")
        </div>
    </div>
</body>
</html>
