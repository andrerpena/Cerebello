@model ConfigAccountViewModel
@{
    var title = "Configurações da conta";
    this.ViewBag.Title = string.Format("{0} · Cerebello", title);
    Layout = "~/Areas/App/Views/Shared/_LayoutPractice.cshtml";
}
@section Title {
    @title @LockIcon.Icon()
}
@section Scripts {
    @LockIcon.Script(LockAccessType.Owner)
}
@section Styles
{
    <link href="@Url.Content("~/Content/Styles/App/PlanPage.css")" rel="Stylesheet" type="text/css" />
}
@Html.Partial((string)string.Format("Index_{0}", this.ViewBag.CurrentContractName), this.Model)
