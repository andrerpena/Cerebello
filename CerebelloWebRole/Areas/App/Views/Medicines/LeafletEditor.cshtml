
@model MedicineLeafletViewModel
@{
    Layout = null;
    var editPanel = this.Html.CreateEditPanel(null);
    editPanel.AddField(m => m.Description);
    editPanel.AddField(m => m.Url);
}
@using (HtmlExtensions.BeginCollectionItem(this.Html, "Leaflets"))
{
    @Html.HiddenFor(model => model.Id)
    @editPanel.GetHtml()
}
